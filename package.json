{
  "name": "react-testing-cookbook",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "https://github.com/andfelzapata/react-testing-book.git",
  "author": "Andres Zapata <andreszapatacano@gmail.com>",
  "license": "MIT",
  "scripts": {
    "lint": "eslint ./src/* --ext .js, .jsx",
    "lint:fix": "eslint ./src/* --ext .js, .jsx --fix",
    "test": "mocha './src/**/*.spec.js' --compilers js:babel-core/register",
    "test:watch": "npm test -- --watch",
    "serve": "webpack-dev-server --colors",
    "dev": "parallelshell 'npm run test:watch' 'npm run serve'"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "css-loader": "^0.28.9",
    "eslint": "^4.16.0",
    "eslint-plugin-react": "^7.6.0",
    "expect": "^22.1.0",
    "expect-jsx": "^5.0.0",
    "file-loader": "^1.1.6",
    "mocha": "^5.0.0",
    "parallelshell": "^3.0.2",
    "react-addons-test-utils": "^15.6.2",
    "react-test-renderer": "^16.2.0",
    "style-loader": "^0.20.1",
    "webpack": "^3.10.0",
    "webpack-dev-server": "^2.11.1"
  },
  "dependencies": {
    "classnames": "^2.2.5",
    "prop-types": "^15.6.0",
    "react": "^16.2.0",
    "react-color": "^2.13.8",
    "react-colors-picker": "^2.3.1",
    "react-dom": "^16.2.0",
    "react-redux": "^5.0.6",
    "react-render": "^1.2.1",
    "redux": "^3.7.2"
  }
}
